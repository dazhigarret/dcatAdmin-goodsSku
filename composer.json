{
    "name": "dazhi/dcatAdmin-goodsSku",
    "description": "基于Dcat Admin的Sku插件,自用",
    "type": "library",
    "keywords": ["dcat-admin", "extension"],
    "homepage": "https://github.com/dazhigarret/dcatAdmin-goodsSku.git",
    "license": "MIT",
    "authors": [
        {
            "name": "dazhi",
            "email": "dazhigarret@163.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dcat/laravel-admin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Dcat\\Admin\\Extension\\EasySku\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Dcat\\Admin\\Extension\\EasySku\\DcatEasySkuServiceProvider",
        "laravel": {
            "providers": [
                "Dcat\\Admin\\Extension\\EasySku\\DcatEasySkuServiceProvider"
            ]
        }
    }
}
